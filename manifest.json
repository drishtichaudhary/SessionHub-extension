{
  "manifest_version": 3,
  "name": "SessionHub",
  "version": "1.0.0",
  "description": "Save and restore your browser tabs with one click",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Session Hub"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
